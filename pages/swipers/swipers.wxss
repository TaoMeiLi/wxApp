.swiper-page {
  width:100;
  padding-top:128rpx;
}
.swiperClass {
  margin: 0;
  margin-top: 50rpx;
}
 
.slide-image {
  width: 100%; 
  height: 100%;
  border-radius: 10rpx;
  position: relative;
}
 
view.active {
  transform: none;
  transition: all 0.2s ease-in 0s;
} 
 
view.quiet {
  transform: scale(0.85);
  transition: all 0.2s ease-in 0s;
}
