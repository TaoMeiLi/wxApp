/* components/confirmBtn/confirmBtn.wxss */

.confirmbtn-component {
  width:100%;
  height: 90rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 0 28rpx;
  overflow: hidden;
  background-color: #EFF0F2;
  font-family: PingFangSC-Medium;
  line-height: 90rpx;
  z-index: 99;
  
}
.btn {
    font-weight: 200;
  }
  .yes-btn {
    float: right;
    color: #067EEE;
  }
  .cancel-btn {
    float: left;
    color: #161717;
  }